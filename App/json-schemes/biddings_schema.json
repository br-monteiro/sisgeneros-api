{
    "type": "object",
    "properties": {
        "number": {
            "type": "integer",
            "minimum": 1,
            "maximum": 99999
        },
        "year": {
            "type": "integer",
            "minimum": 2018,
            "maximum": 2030
        },
        "uasgNumber": {
            "type": "integer",
            "minimum": 1,
            "maximum": 999999
        },
        "uasgName": {
            "type": "string",
            "minLength": 3,
            "maxLength": 100
        },
        "validate": {
            "type": "string",
            "pattern": "^20[12][0-9]-[01][0-2]-[0-3][0-9]$"
        }
    },
    "required": ["number", "year", "uasgNumber", "uasgName", "validate"]
}